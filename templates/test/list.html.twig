{% extends 'base.html.twig' %}

{% block title %}Programovací jazyk!{% endblock %}

{% block body %}

<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Programovací jazyky</h1>


    <table class="table">
        {% for language in languanges %}
        <tr>
            <td>{{ language.testString }}</td>
            <td>{% if language.testBoolean == 1 %} ano
            {% else %}
            ne
                {% endif %}
            </td>
        </tr>
        {% endfor %}
    </table>

</div>

    <a href="{{ path ('know', {'name':'know'}) }}">Know</a>

</div>
{% endblock %}

